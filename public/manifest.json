{
  "manifest_version": 3,
  "name": "Techstack Demo Chat Bot",
  "description": "Chrome extension for frontend of Chat Bot using React",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    {
      "resources": ["chatbot.svg", "scissor.png", "pin.png", "history.png", "chat.png", "tool.png"],
      "matches": ["<all_urls>"]
    }
  ]
}